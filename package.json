{
  "name": "enkhee-osiris.github.io",
  "version": "1.0.1",
  "description": "enkhee-Osiris's personal websites",
  "private": true,
  "scripts": {
    "format": "npm run pretty --write && npm run lint -- -- -- --fix",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "pretty": "prettier -c -u ./",
    "build": "turbo run build export",
    "deploy": "./packages/scripts/export.js && ./packages/scripts/deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enkhee-Osiris/enkhee-osiris.github.io.git"
  },
  "keywords": [
    "enkhee-Osiris",
    "blog",
    "resume"
  ],
  "author": "enkhee-Osiris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enkhee-Osiris/enkhee-osiris.github.io/issues"
  },
  "homepage": "https://github.com/enkhee-Osiris/enkhee-osiris.github.io#readme",
  "workspaces": [
    "packages/*",
    "sites/*"
  ],
  "dependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-aspect-ratio": "^0.1.5-rc.3",
    "@stitches/react": "^1.2.7",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "usehooks-ts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "18.0.0",
    "next-transpile-modules": "^9.0.0",
    "prettier": "^2.6.2",
    "turbo": "^1.2.1",
    "typescript": "4.6.3"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "npm@8.5.4"
}
